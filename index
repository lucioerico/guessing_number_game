var num;

numb = Math.floor (Math.random() * 10);
alert (numb)

do {
chute = prompt("Insira um número entre 0 e 9");
} while (chute < 0 || chute > 9);

for (var cont = 1; chute != numb; cont++){
  do {
chute = prompt("Insira um número entre 0 e 9");
} while (chute < 0 || chute > 9);
}
alert ("você acertou na tentativa " + cont)
